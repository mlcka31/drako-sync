  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "debug",
      "name": "Debug",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "node",
      "runtimeArgs": ["--require=dotenv/config", "--require=ts-node/register"],
      "env": {
        "DOTENV_CONFIG_PATH": "${workspaceFolder}/backend-2/.env"
      },
      "cwd": "${workspaceFolder}/backend-2",
      "console": "integratedTerminal",
      "restart": true,
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}
